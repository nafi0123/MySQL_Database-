CREATE TABLE Sylhet_tour
(
    Roll int,
    Name varchar(15),
    Gender varchar(10),
    Age int(3),
    PRIMARY KEY(Roll)
);

CREATE TABLE Dhaka_tour
(
    Roll int,
    Name varchar(15),
    Gender varchar(10),
    Age int(3),
    PRIMARY KEY(Roll)
);

